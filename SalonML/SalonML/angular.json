{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "SalonML": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/SalonML",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1600kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "34kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "SalonML:build:production"
            },
            "development": {
              "browserTarget": "SalonML:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "src/proxy.conf.js"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "SalonML:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/app/shared/vendor/js/bundle.min.js",
              "src/app/shared/vendor/js/jquery.appear.js",
              "src/app/shared/vendor/js/jquery.fancybox.min.js",
              "src/app/shared/vendor/js/owl.carousel.min.js",
              "src/app/shared/vendor/js/parallaxie.min.js",
              "src/app/shared/vendor/js/wow.min.js",
              "src/app/shared/vendor/js/jquery.cubeportfolio.min.js",
              "src/app/shared/vendor/js/jquery.themepunch.tools.min.js",
              "src/app/shared/vendor/js/jquery.themepunch.revolution.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.actions.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.carousel.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.kenburn.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.layeranimation.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.migration.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.navigation.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.parallax.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.slideanims.min.js",
              "src/app/shared/vendor/js/extensions/revolution.extension.video.min.js",
              "src/app/shared/resume/js/map.js",
              "src/app/shared/vendor/js/contact_us.js",
              "src/app/shared/resume/js/script.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "3a870b8d-aeb3-432d-be61-3d7fd7280a35"
  }
}
